{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "../src/*.sln",
                        "../src/**/*.csproj",
                        "../src/ProjectOrigin.Electricity.Client/ProjectOrigin.Electricity.Client.csproj"
                    ]
                }
            ],
            "dest": "obj/api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.yml"
                ],
                "src": "obj/api",
                "dest": "api"
            },
            {
                "files": [
                    "**/toc.yml",
                    "**/*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "**/*.svg"
                ]
            }
        ],
        "globalMetadata": {
            "_appTitle": "Project Origin",
            "_enableSearch": true
        },
        "markdownEngineName": "markdig",
        "dest": "_site"
    }
}
